
//8 CSN
//7 CE
//13 SCK
//11 MOSI
//12 MISO

#include <SPI.h>
#include <nRF24L01.h>
#include <RF24.h>

RF24 radio(7,8);
const byte address[6]="00001";

void setup(){

Serial.begin(9600);
  radio.begin();
  radio.openWritingPipe(address);
  radio.setPALevel(RF24_PA_MIN);
  radio.stopListening();
  
}

void loop(){
  
Serial.println("Hello");
const char text[]="Hello";
radio.write(&text,sizeof(text));
delay(1000);
  
}